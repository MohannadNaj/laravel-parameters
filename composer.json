{
    "name": "mohannadnaj/paramters",
    "description": "Helper Package for marking active state in application's runtime.",
    "license": "MIT",
    "type": "package",
    "homepage": "https://github.com/mohannadnaj/active-state",
    "authors": [
      {
        "name": "Mohannad Najjar",
        "email": "mohannad.najjar@mail.com"
      }
    ],
    "keywords": ["framework", "laravel"],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "~5.4.0",
        "illuminate/http": "~5.4.0",
        "illuminate/support": "~5.4.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7",
        "orchestra/database": "~3.4",
        "orchestra/testbench-browser-kit": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Parameter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parameter\\": "src/",
            "Parameter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parameter\\Providers\\ParametersServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
